# Gregory Trading Agent - Environment Variables
# Скопируйте в .env и заполните ваши значения

# PostgreSQL
POSTGRES_PASSWORD=gregory_secret_change_me

# n8n
N8N_USER=admin
N8N_PASSWORD=n8n_secret_change_me
N8N_ENCRYPTION_KEY=n8n_encryption_key_32_chars_min

# Webhooks
N8N_WEBHOOK_SECRET=webhook_secret_change_me

# Gregory API
DATABASE_URL=postgresql://gregory:gregory_secret_change_me@postgres:5432/gregory_orchestration
ARTIFACTS_ROOT=/app/artifacts

# External APIs
FUNDINGPIPS_API_KEY=your_fundingpips_key
FUNDINGPIPS_SECRET=your_fundingpips_secret
HASHHEDGE_API_KEY=your_hashhedge_key
HASHHEDGE_SECRET=your_hashhedge_secret

# Telegram
TELEGRAM_BOT_TOKEN=your_telegram_bot_token
TELEGRAM_CHAT_ID=your_telegram_chat_id

# Optional: S3 for artifacts (future use)
# S3_ENDPOINT=
# S3_BUCKET=
# S3_ACCESS_KEY=
# S3_SECRET_KEY=
